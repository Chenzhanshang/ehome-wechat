/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.bar{
  width: 100%;
  height: 5px;
  background: #eee;
}
.hr{
  width: 100%;
  height: 1px;
  background: #eee;
}
.con-row{
  display: flex;
  flex-direction: row;
}
.con-column{
  display: flex;
  flex-direction: column;
}