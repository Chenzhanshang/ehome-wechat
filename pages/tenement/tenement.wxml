<view class="main">
  <view class="main-box">
    <!-- 顶部框 -->
    <view class="top-box">
      <view class="bg-img">
        <view class="tap" bindtap="shenqingruzhu">
          <image src="../bgImg/sqrz.png" style="width:14px;height:14px;"></image>
          <text class="sqrz-text">申请入驻</text>
        </view>
      </view>
      <view class="bg-img-bot">
        <image src="../bgImg/wy.jpeg" style="width:100%;height:100px"></image>
      </view>
    </view>
    <view class="bar"></view>
    <!-- 功能栏 -->
  <navigator  wx:for="{{funArr}}" wx:key="index"  class="weui-cell weui-cell_example weui-cell_access" hover-class="weui-cell_active">
    <view class="weui-cell__hd">
       <image src="{{item.url}}" style="width: 20px;height: 20px;margin-right: 16px;vertical-align:middle;" />
        </view>
        <view class="weui-cell__bd weui-cell_primary">
            <view>{{item.title}}</view>
        </view>
        <view class="weui-cell__ft weui-cell__ft_in-access"></view>
    </navigator>

  </view>
</view>

